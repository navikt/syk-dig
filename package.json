{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.12.0",
    "engines": {
        "node": "24"
    },
    "scripts": {
        "dev": "concurrently --names 'next,gen'  --prefix-colors 'blue,magenta' 'yarn dev:next' 'yarn gen:watch'",
        "dev:next": "next dev | pino-pretty --messageKey=message",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty --messageKey=message",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "lint": "eslint src",
        "test": "vitest --config vitest.config.mts",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "gen": "graphql-codegen",
        "gen:watch": "graphql-codegen --watch",
        "pull-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "update-schema": "yarn pull-schema && yarn gen",
        "prepare": "husky"
    },
    "dependencies": {
        "@apollo/client": "^4.0.10",
        "@ariakit/react": "^0.4.20",
        "@graphql-tools/schema": "^10.0.30",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^7.35.2",
        "@navikt/diagnosekoder": "1.2025.0",
        "@navikt/ds-css": "^7.35.2",
        "@navikt/ds-react": "^7.35.2",
        "@navikt/next-api-proxy": "^5.0.2",
        "@navikt/next-logger": "^4.4.4",
        "@navikt/oasis": "^4.1.1",
        "@unleash/nextjs": "^1.6.2",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.1.0",
        "graphql": "^16.12.0",
        "graphql-yoga": "^5.17.1",
        "next": "16.1.5",
        "next-logger": "^5.0.2",
        "nextleton": "^0.6.1",
        "node-cache": "^5.1.2",
        "pdfobject": "^2.3.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "prom-client": "^15.1.3",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-hook-form": "^7.68.0",
        "react-resizable-panels": "^3.0.6",
        "remeda": "^2.32.0",
        "rxjs": "^7.8.2",
        "swr": "^2.3.7",
        "tailwind-merge": "^3.4.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^6.0.0",
        "@graphql-codegen/cli": "^6.1.0",
        "@graphql-codegen/fragment-matcher": "^6.0.0",
        "@graphql-codegen/typed-document-node": "^6.1.4",
        "@graphql-codegen/typescript-operations": "^5.0.6",
        "@graphql-codegen/typescript-resolvers": "^5.1.4",
        "@hookform/devtools": "^4.4.0",
        "@navikt/ds-tailwind": "^7.35.2",
        "@navikt/tsm-eslint-react": "^1.0.0",
        "@navikt/tsm-prettier": "^2.0.0",
        "@parcel/watcher": "^2.5.1",
        "@playwright/test": "1.57.0",
        "@tailwindcss/postcss": "^4.1.18",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "24.10.3",
        "@types/pdfobject": "^2.2.5",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.3",
        "eslint": "9.39.2",
        "eslint-config-next": "16.0.10",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "graphql-sock": "^1.0.1",
        "husky": "^9.1.7",
        "jsdom": "^27.3.0",
        "lint-staged": "^16.2.7",
        "next-router-mock": "^1.0.4",
        "nock": "^14.0.10",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "tailwindcss": "^4.1.18",
        "typescript": "5.9.3",
        "vite": "^7.2.7",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.15",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "prettier": "@navikt/tsm-prettier"
}
