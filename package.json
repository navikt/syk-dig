{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "jest",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen -w --config codegen.ts",
        "update-schema": "wget https://syk-dig-backend.dev.intern.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.7.10",
        "@hookform/error-message": "^2.0.1",
        "@navikt/ds-css": "^2.8.2",
        "@navikt/ds-css-internal": "^2.8.2",
        "@navikt/ds-icons": "^2.8.2",
        "@navikt/ds-react": "^2.8.2",
        "@navikt/ds-react-internal": "^2.8.2",
        "@navikt/next-api-proxy": "^2.1.0",
        "@navikt/next-auth-wonderwall": "^1.2.0",
        "@navikt/next-logger": "^1.14.0",
        "ariakit": "^2.0.0-next.43",
        "clsx": "^1.2.1",
        "fuse.js": "^6.6.2",
        "graphql": "^16.6.0",
        "jest-axe": "^7.0.0",
        "next": "13.2.4",
        "next-logger": "^3.0.1",
        "pdfobject": "^2.2.8",
        "pino": "^8.11.0",
        "pino-pretty": "^10.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.43.8",
        "remeda": "^1.9.1",
        "tailwind-merge": "^1.10.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^4.0.1",
        "@graphql-codegen/cli": "^3.2.2",
        "@graphql-codegen/fragment-matcher": "^4.0.1",
        "@graphql-codegen/typed-document-node": "^3.0.2",
        "@graphql-codegen/typescript-operations": "^3.0.2",
        "@navikt/ds-tailwind": "^2.8.2",
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest-axe": "^3.5.5",
        "@types/node": "18.15.5",
        "@types/pdfobject": "^2.2.3",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@types/testing-library__jest-dom": "^5.14.5",
        "autoprefixer": "^10.4.14",
        "concurrently": "^7.6.0",
        "eslint": "8.36.0",
        "eslint-config-next": "13.2.4",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "^13.2.0",
        "msw": "1.1.1",
        "next-router-mock": "^0.9.2",
        "open": "^8.4.2",
        "postcss": "^8.4.21",
        "postcss-import": "^15.1.0",
        "prettier": "^2.8.6",
        "prettier-plugin-tailwindcss": "^0.2.5",
        "tailwindcss": "^3.2.7",
        "typescript": "5.0.2"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    }
}
