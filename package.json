{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "scripts": {
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "jest",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen -w --config codegen.ts",
        "update-schema": "wget https://syk-dig-backend.dev.intern.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.7.1",
        "@hookform/error-message": "^2.0.0",
        "@navikt/ds-css": "^1.4.0",
        "@navikt/ds-css-internal": "^1.4.0",
        "@navikt/ds-icons": "^1.4.0",
        "@navikt/ds-react": "^1.4.0",
        "@navikt/ds-react-internal": "^1.4.0",
        "@navikt/next-api-proxy": "^2.0.0",
        "@navikt/next-auth-wonderwall": "^0.8.0",
        "@navikt/next-logger": "^1.11.0",
        "ariakit": "^2.0.0-next.41",
        "clsx": "^1.2.1",
        "fuse.js": "^6.6.2",
        "graphql": "^16.6.0",
        "jest-axe": "^6.0.0",
        "next": "13.0.0",
        "next-logger": "^3.0.1",
        "pdfobject": "^2.2.8",
        "pino": "^8.7.0",
        "pino-pretty": "^9.1.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.38.0",
        "remeda": "^1.2.0",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^3.2.1",
        "@graphql-codegen/cli": "^2.13.8",
        "@graphql-codegen/fragment-matcher": "^3.3.1",
        "@graphql-codegen/typed-document-node": "^2.3.5",
        "@graphql-codegen/typescript-operations": "^2.5.5",
        "@navikt/eslint-config-teamsykmelding": "^1.0.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest-axe": "^3.5.5",
        "@types/node": "18.11.8",
        "@types/pdfobject": "^2.2.3",
        "@types/react": "18.0.24",
        "@types/react-dom": "18.0.8",
        "@types/testing-library__jest-dom": "^5.14.5",
        "concurrently": "^7.5.0",
        "eslint": "8.26.0",
        "eslint-config-next": "13.0.0",
        "husky": "^8.0.1",
        "jest": "^29.2.2",
        "jest-environment-jsdom": "^29.2.2",
        "lint-staged": "^13.0.3",
        "msw": "0.47.4",
        "next-router-mock": "^0.7.4",
        "open": "^8.4.0",
        "prettier": "^2.7.1",
        "typescript": "4.8.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
