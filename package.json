{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.1.1",
    "engines": {
        "node": "20"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "vitest --config vitest.config.mts",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen --watch --config codegen.ts",
        "pull-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "update-schema": "yarn pull-schema && yarn gen",
        "prepare": "husky"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.10.3",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^6.7.0",
        "@navikt/diagnosekoder": "1.2024.0",
        "@navikt/ds-css": "^6.7.0",
        "@navikt/ds-react": "^6.7.0",
        "@navikt/next-api-proxy": "^3.4.1",
        "@navikt/next-logger": "^1.24.0",
        "@navikt/oasis": "^3.2.4",
        "@next/bundle-analyzer": "14.2.3",
        "@unleash/nextjs": "^1.4.2",
        "ariakit": "^2.0.0-next.44",
        "clsx": "^2.1.1",
        "date-fns": "^3.6.0",
        "fuse.js": "^7.0.0",
        "graphql": "^16.8.1",
        "next": "14.2.3",
        "next-logger": "^4.0.0",
        "nextleton": "^0.6.1",
        "pdfobject": "^2.3.0",
        "pino": "^9.0.0",
        "pino-pretty": "^11.0.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "^7.51.4",
        "remeda": "^1.61.0",
        "tailwind-merge": "^2.3.0",
        "zod": "^3.23.7"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.2",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/fragment-matcher": "^5.0.2",
        "@graphql-codegen/typed-document-node": "^5.0.6",
        "@graphql-codegen/typescript-operations": "^4.2.0",
        "@navikt/ds-tailwind": "^6.7.0",
        "@navikt/eslint-config-teamsykmelding": "^6.2.0",
        "@parcel/watcher": "^2.4.1",
        "@testing-library/dom": "^10.1.0",
        "@testing-library/react": "^15.0.7",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "20.12.10",
        "@types/pdfobject": "^2.2.5",
        "@types/react": "18.3.1",
        "@types/react-dom": "18.3.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "eslint": "8.57.0",
        "eslint-config-next": "14.2.3",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.2",
        "msw": "2.2.14",
        "next-router-mock": "^0.9.13",
        "open": "^10.1.0",
        "postcss": "^8.4.38",
        "postcss-import": "^16.1.0",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "tailwindcss": "^3.4.3",
        "typescript": "5.4.5",
        "vite": "^5.2.12",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "resolutions": {
        "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
