{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "engines": {
        "node": ">=18.17"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "vitest --config ./vitest.config.ts",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen -w --config codegen.ts",
        "update-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.8.4",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^5.6.3",
        "@navikt/ds-css": "^5.6.3",
        "@navikt/ds-react": "^5.6.3",
        "@navikt/next-api-proxy": "^3.3.0",
        "@navikt/next-auth-wonderwall": "^1.5.0",
        "@navikt/next-logger": "^1.19.0",
        "@next/bundle-analyzer": "13.4.12",
        "ariakit": "^2.0.0-next.44",
        "clsx": "^2.0.0",
        "fuse.js": "^6.6.2",
        "graphql": "^16.8.1",
        "next": "13.4.12",
        "next-logger": "^3.0.2",
        "nextleton": "^0.6.1",
        "pdfobject": "^2.2.12",
        "pino": "^8.15.1",
        "pino-pretty": "^10.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.46.2",
        "remeda": "^1.27.0",
        "tailwind-merge": "^1.14.0",
        "zod": "^3.22.2"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.0",
        "@graphql-codegen/cli": "^5.0.0",
        "@graphql-codegen/fragment-matcher": "^5.0.0",
        "@graphql-codegen/typed-document-node": "^5.0.1",
        "@graphql-codegen/typescript-operations": "^4.0.1",
        "@navikt/ds-tailwind": "^5.6.3",
        "@navikt/eslint-config-teamsykmelding": "^4.3.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.5.1",
        "@types/node": "18.17.18",
        "@types/pdfobject": "^2.2.3",
        "@types/react": "18.2.22",
        "@types/react-dom": "18.2.7",
        "@vitejs/plugin-react": "^4.0.4",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.1",
        "dotenv": "^16.3.1",
        "eslint": "8.50.0",
        "eslint-config-next": "13.4.12",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^14.0.1",
        "msw": "1.3.1",
        "next-router-mock": "^0.9.9",
        "open": "^9.1.0",
        "postcss": "^8.4.30",
        "postcss-import": "^15.1.0",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "tailwindcss": "^3.3.3",
        "typescript": "5.2.2",
        "vitest": "^0.34.5",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "resolutions": {
        "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
