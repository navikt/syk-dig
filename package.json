{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.6.0",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "lint": "next lint",
        "test": "vitest --config vitest.config.mts",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen --watch --config codegen.ts",
        "pull-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "update-schema": "yarn pull-schema && yarn gen",
        "prepare": "husky"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "3.11.8",
        "@ariakit/react": "^0.4.15",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^7.9.1",
        "@navikt/diagnosekoder": "1.2025.0",
        "@navikt/ds-css": "^7.9.1",
        "@navikt/ds-react": "^7.9.1",
        "@navikt/next-api-proxy": "^4.1.3",
        "@navikt/next-logger": "^1.35.0",
        "@navikt/oasis": "^3.6.0",
        "@unleash/nextjs": "^1.5.0",
        "apollo-link-rest": "^0.9.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.0.0",
        "graphql": "^16.10.0",
        "next": "15.1.4",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cache": "^5.1.2",
        "pdfobject": "^2.3.0",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0",
        "qs": "^6.13.1",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "^7.54.2",
        "react-resizable-panels": "^2.1.7",
        "remeda": "^2.19.0",
        "swr": "^2.3.0",
        "tailwind-merge": "^2.6.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.3",
        "@graphql-codegen/cli": "^5.0.3",
        "@graphql-codegen/fragment-matcher": "^5.0.2",
        "@graphql-codegen/typed-document-node": "^5.0.12",
        "@graphql-codegen/typescript-operations": "^4.4.0",
        "@hookform/devtools": "^4.3.3",
        "@navikt/ds-tailwind": "^7.9.1",
        "@navikt/eslint-config-teamsykmelding": "^7.1.0",
        "@parcel/watcher": "^2.5.0",
        "@playwright/test": "1.49.1",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.1.0",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "22.10.5",
        "@types/pdfobject": "^2.2.5",
        "@types/qs": "^6.9.17",
        "@types/react": "^19.0.3",
        "@types/react-dom": "19.0.2",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.2",
        "dotenv": "^16.4.7",
        "eslint": "8.57.1",
        "eslint-config-next": "15.1.4",
        "husky": "^9.1.7",
        "jsdom": "^25.0.1",
        "lint-staged": "^15.3.0",
        "msw": "2.5.1",
        "next-router-mock": "^0.9.13",
        "open": "^10.1.0",
        "postcss": "^8.4.49",
        "postcss-import": "^16.1.0",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "tailwindcss": "^3.4.17",
        "typescript": "5.6.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^2.1.8",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
