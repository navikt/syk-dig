{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@3.2.3",
    "engines": {
        "node": "18"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "jest",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen -w --config codegen.ts",
        "update-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "postinstall": "node ./scripts/postinstall.js"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.7.14",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^3.2.3",
        "@navikt/ds-css": "^3.2.3",
        "@navikt/ds-css-internal": "^3.2.3",
        "@navikt/ds-icons": "^3.2.3",
        "@navikt/ds-react": "^3.2.3",
        "@navikt/ds-react-internal": "^3.2.3",
        "@navikt/next-api-proxy": "^3.2.0",
        "@navikt/next-auth-wonderwall": "^1.3.0",
        "@navikt/next-logger": "^1.19.0",
        "ariakit": "^2.0.0-next.43",
        "clsx": "^1.2.1",
        "fuse.js": "^6.6.2",
        "graphql": "^16.6.0",
        "jest-axe": "^7.0.1",
        "next": "^13.4.3",
        "next-logger": "^3.0.2",
        "pdfobject": "^2.2.11",
        "pino": "^8.14.1",
        "pino-pretty": "^10.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.43.9",
        "remeda": "^1.18.0",
        "tailwind-merge": "^1.12.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^4.0.1",
        "@graphql-codegen/cli": "^3.3.1",
        "@graphql-codegen/fragment-matcher": "^4.0.1",
        "@graphql-codegen/typed-document-node": "^4.0.1",
        "@graphql-codegen/typescript-operations": "^3.0.4",
        "@navikt/ds-tailwind": "^3.2.3",
        "@navikt/eslint-config-teamsykmelding": "^1.6.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest-axe": "^3.5.5",
        "@types/node": "18.16.14",
        "@types/pdfobject": "^2.2.3",
        "@types/react": "18.0.38",
        "@types/react-dom": "18.0.11",
        "@types/testing-library__jest-dom": "^5.14.5",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.0.1",
        "eslint": "8.41.0",
        "eslint-config-next": "13.4.3",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "^13.2.2",
        "msw": "1.2.1",
        "next-router-mock": "^0.9.3",
        "open": "^8.4.2",
        "postcss": "^8.4.23",
        "postcss-import": "^15.1.0",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "tailwindcss": "^3.3.2",
        "typescript": "5.0.4"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ],
        "rules": {
            "postcss-modules/no-undef-class": "off",
            "postcss-modules/no-unused-class": "off"
        }
    }
}
