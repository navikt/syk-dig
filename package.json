{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.0.2",
    "engines": {
        "node": "20"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty",
        "build": "next build",
        "lint": "next lint",
        "test": "vitest --config ./vitest.config.ts",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen --watch --config codegen.ts",
        "pull-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "update-schema": "yarn pull-schema && yarn gen",
        "prepare": "husky"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "dependencies": {
        "@apollo/client": "^3.9.5",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^5.18.3",
        "@navikt/diagnosekoder": "1.2024.0",
        "@navikt/ds-css": "^5.18.3",
        "@navikt/ds-react": "^5.18.3",
        "@navikt/next-api-proxy": "^3.4.0",
        "@navikt/next-logger": "^1.23.0",
        "@navikt/oasis": "^3.2.1",
        "@next/bundle-analyzer": "14.1.0",
        "@unleash/nextjs": "^1.4.0",
        "ariakit": "^2.0.0-next.44",
        "clsx": "^2.1.0",
        "fuse.js": "^7.0.0",
        "graphql": "^16.8.1",
        "next": "14.1.0",
        "next-logger": "^3.0.2",
        "nextleton": "^0.6.1",
        "pdfobject": "^2.3.0",
        "pino": "^8.19.0",
        "pino-pretty": "^10.3.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.50.1",
        "remeda": "^1.44.1",
        "tailwind-merge": "^2.2.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^5.0.2",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/fragment-matcher": "^5.0.2",
        "@graphql-codegen/typed-document-node": "^5.0.6",
        "@graphql-codegen/typescript-operations": "^4.2.0",
        "@navikt/ds-tailwind": "^5.18.3",
        "@navikt/eslint-config-teamsykmelding": "^5.2.0",
        "@parcel/watcher": "^2.4.1",
        "@testing-library/react": "^14.2.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/node": "20.11.21",
        "@types/pdfobject": "^2.2.5",
        "@types/react": "18.2.60",
        "@types/react-dom": "18.2.19",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.17",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "eslint": "8.57.0",
        "eslint-config-next": "14.1.0",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.2",
        "msw": "2.2.2",
        "next-router-mock": "^0.9.12",
        "open": "^10.0.4",
        "postcss": "^8.4.35",
        "postcss-import": "^16.0.1",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.11",
        "tailwindcss": "^3.4.1",
        "typescript": "5.3.3",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.3.1",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "resolutions": {
        "eslint-plugin-react-hooks": "5.0.0-canary-7118f5dd7-20230705"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier",
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals"
        ]
    }
}
