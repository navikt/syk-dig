{
    "name": "syk-dig",
    "version": "0.1.0",
    "private": true,
    "packageManager": "yarn@4.5.1",
    "engines": {
        "node": "22"
    },
    "scripts": {
        "dev": "yarn start",
        "start": "concurrently -n next,codegen \"yarn start:watch\" \"yarn gen:watch\"",
        "start:watch": "next dev | pino-pretty --messageKey=message",
        "start:e2e": "NEXT_PUBLIC_ASSET_PREFIX= node .next/standalone/server.js | pino-pretty --messageKey=message",
        "build": "./scripts/pre-build.sh && next build",
        "build:e2e": "./scripts/build-e2e.sh",
        "lint": "eslint src",
        "test": "vitest --config vitest.config.mts",
        "e2e": "playwright test",
        "e2e:dev": "yarn e2e --ui",
        "e2e:fast": "FAST=true yarn e2e --ui",
        "gen": "graphql-codegen --config codegen.ts",
        "gen:watch": "graphql-codegen --watch --config codegen.ts",
        "pull-schema": "wget https://syk-dig-backend.intern.dev.nav.no/schema.json -O ./schema.json && prettier -w schema.json",
        "update-schema": "yarn pull-schema && yarn gen",
        "prepare": "husky"
    },
    "dependencies": {
        "@apollo/client": "^3.14.0",
        "@ariakit/react": "^0.4.18",
        "@graphql-tools/schema": "^10.0.25",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@hookform/error-message": "^2.0.1",
        "@navikt/aksel-icons": "^7.31.0",
        "@navikt/diagnosekoder": "1.2025.0",
        "@navikt/ds-css": "^7.31.0",
        "@navikt/ds-react": "^7.31.0",
        "@navikt/next-api-proxy": "^4.1.4",
        "@navikt/next-logger": "^4.1.5",
        "@navikt/oasis": "^3.8.0",
        "@unleash/nextjs": "^1.6.2",
        "apollo-link-rest": "^0.9.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "fuse.js": "^7.1.0",
        "graphql": "^16.11.0",
        "graphql-yoga": "^5.16.0",
        "next": "^15.5.4",
        "next-logger": "^5.0.1",
        "nextleton": "^0.6.1",
        "node-cache": "^5.1.2",
        "pdfobject": "^2.3.1",
        "pino": "^9.12.0",
        "pino-pretty": "^13.1.1",
        "qs": "^6.14.0",
        "react": "19.1.1",
        "react-dom": "19.1.1",
        "react-hook-form": "^7.63.0",
        "react-resizable-panels": "^2.1.9",
        "remeda": "^2.32.0",
        "swr": "^2.3.6",
        "tailwind-merge": "^2.6.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@graphql-codegen/add": "^6.0.0",
        "@graphql-codegen/cli": "^6.0.0",
        "@graphql-codegen/fragment-matcher": "^6.0.0",
        "@graphql-codegen/typed-document-node": "^6.0.1",
        "@graphql-codegen/typescript-operations": "^5.0.1",
        "@graphql-codegen/typescript-resolvers": "^5.0.1",
        "@hookform/devtools": "^4.4.0",
        "@navikt/ds-tailwind": "^7.31.0",
        "@navikt/eslint-config-teamsykmelding": "^8.1.1",
        "@parcel/watcher": "^2.5.1",
        "@playwright/test": "1.55.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "22.18.6",
        "@types/pdfobject": "^2.2.5",
        "@types/react": "^19.1.15",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.4",
        "autoprefixer": "^10.4.21",
        "concurrently": "^9.2.1",
        "dotenv": "^17.2.2",
        "eslint": "9.36.0",
        "eslint-config-next": "^15.5.4",
        "graphql-sock": "^1.0.1",
        "husky": "^9.1.7",
        "jsdom": "^27.0.0",
        "lint-staged": "^16.2.3",
        "next-router-mock": "^1.0.2",
        "nock": "^14.0.10",
        "postcss": "^8.5.6",
        "postcss-import": "^16.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^3.4.17",
        "typescript": "5.9.2",
        "vite": "^7.1.7",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4",
        "vitest-axe": "^0.1.0",
        "vitest-dom": "^0.1.1"
    },
    "resolutions": {
        "@types/react": "npm:types-react@19.0.0-rc.1",
        "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
